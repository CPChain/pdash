QLabel {
    color: #fefefe;
}

QDialog{
    background: #fafafa;
}

QLabel#choice_label{
    color: #414141;
    font: bold 15px;
}

QLineEdit#search_bar {
    border-radius: 5px;
    border-color: #d7d7d7;
}

