QPushButton#login_btn{
    padding-left: 14px;
    padding-right: 14px;
    border: 1px solid #3173d8; 
    border-radius: 5px;
    color: #ffffff;
    min-height: 30px;
    max-height: 30px;
    background: #3173d8;
}

QPushButton#login_btn:hover{
    background: #3984f7; 
    border: 1px solid #3984f7;
}

QPushButton#login_btn:pressed{
    border: 1px solid #2e6dcd; 
    background: #2e6dcd;
}

QPushbutton#login_btn:disabled{
    border: 1px solid #8cb8ea; 
    background: #98b9eb;
}

QPushButton#profile_page_btn:pressed {
    background: #5ca7ee;
}

QPushButton#profile_page_btn {
    min-height: 35px; 
    min-width: 35px;  
    max-width: 35px; 
    max-height: 35px;
    border-image: url(${asset_dir}/icons/profile.png);
    background-color: #006bcf;
}

QPushButton#profile_btn {
    min-height: 5px; 
    min-width: 8px;  
    max-width: 8px; 
    max-height: 5px;
    border-image: url(${asset_dir}/icons/profile_arrow.png);
    background-color: #006bcf;
}

QPushButton#cancel_btn{
    padding-left: 14px;
    padding-right: 14px;
    border: 1px solid #3173d8; 
    border-radius: 5px;
    color: #3173d8;
    min-height: 30px;
    max-height: 30px;
    background: #ffffff;
}

QPushButton#cancel_btn:hover{
    border: 1px solid #3984f7; 
    color: #3984f6;
}

QPushButton#cancel_btn:pressed{
    border: 1px solid #2e6dcd; 
    color: #2e6dcd;
    background: #e5ecf4;
}

QPushbutton#cancel_btn:disabled{
    border: 1px solid #8cb8ea; 
    color: #8cb8ea;
}




